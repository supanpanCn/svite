#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+glob@10.3.10/node_modules/glob/dist/esm/bin.mjs" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+glob@10.3.10/node_modules/glob/dist/esm/bin.mjs" "$@"
fi
